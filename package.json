  "scripts": {
    "build": "tsc -b && vite build --mode production && shx cp ./dist/index.html ./dist/404.html",
    "dev": "vite",
    "postinstall": "husky init && shx rm .husky/pre-commit && shx echo 'npx --no -- commitlint --edit $1' > .husky/commit-msg",
    "prepare": "husky",
    "preview": "tsc -b && vite build && shx cp ./dist/index.html ./dist/404.html && vite preview"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
